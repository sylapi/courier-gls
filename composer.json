{
    "name": "sylapi/courier-gls",
    "type": "library",
    "description": "Courier",
    "keywords": [
        "courier",
        "gls",
        "shipping"
    ],
    "homepage": "https://github.com/sylapi/courier-gls",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Zapotoczny"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "sylapi/courier": "@dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../courier/"
        }
    ],    
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": { "Sylapi\\Courier\\Gls\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Sylapi\\Courier\\Gls\\Tests\\": "tests/" }
    },     
    "prefer-stable": false
}
